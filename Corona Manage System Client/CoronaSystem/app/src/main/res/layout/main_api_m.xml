<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="ExtraText">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/map_View"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="177dp"
        tools:layout_editor_absoluteY="282dp">

        <Button
            android:id="@+id/btn_search_exit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="1dp"
            android:layout_marginBottom="16dp"
            android:enabled="false"
            android:soundEffectsEnabled="true"
            android:background="@drawable/exit"
            android:visibility="invisible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/btn_search_next"
            app:layout_constraintStart_toEndOf="@+id/btn_search_prev" />

        <Button
            android:id="@+id/btn_search_next"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="16dp"
            android:enabled="false"
            android:soundEffectsEnabled="true"
            android:background="@drawable/next"
            android:visibility="invisible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/btn_search_exit" />

        <Button
            android:id="@+id/btn_search_prev"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginBottom="16dp"
            android:enabled="false"
            android:soundEffectsEnabled="true"
            android:background="@drawable/previous"
            android:visibility="invisible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/btn_search_exit"
            app:layout_constraintHorizontal_chainStyle="spread_inside"
            app:layout_constraintStart_toStartOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageView
        android:id="@+id/home_title"
        android:layout_width="330dp"
        android:layout_height="105dp"
        android:layout_marginStart="67dp"
        android:layout_marginEnd="27dp"
        android:layout_marginBottom="626dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.658"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/home_text"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/count"
        android:layout_width="222dp"
        android:layout_height="132dp"
        android:layout_marginStart="198dp"
        android:layout_marginTop="567dp"
        android:layout_marginBottom="32dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/home_count"
        tools:ignore="MissingConstraints" />


    <TextView
        android:id="@+id/tv_today"
        android:layout_width="61dp"
        android:layout_height="36dp"
        android:layout_marginStart="321dp"
        android:layout_marginTop="608dp"
        android:layout_marginEnd="29dp"
        android:layout_marginBottom="87dp"
        android:gravity="center|left"
        android:text="10"
        android:textColor="@color/colorBlack"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_total"
        android:layout_width="61dp"
        android:layout_height="36dp"
        android:layout_marginStart="322dp"
        android:layout_marginTop="644dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="51dp"
        android:gravity="center|left"
        android:text="10"
        android:textColor="@color/colorBlack"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btn_find_location"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="354dp"
        android:layout_marginTop="132dp"
        android:layout_marginEnd="17dp"
        android:layout_marginBottom="559dp"
        android:background="@drawable/location_img"
        android:soundEffectsEnabled="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btn_find_date"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:layout_marginStart="354dp"
        android:layout_marginTop="73dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="616dp"
        android:background="@drawable/date_check_img"
        android:soundEffectsEnabled="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>